// Place your key bindings in this file to overwrite the defaults
[
  {"key": "ctrl+shift+d",
    "command": "workbench.action.tasks.runTask",
    "args": "deploy"
  },
  {
    "key": "ctrl+b shift+'",
    "command": "workbench.action.splitEditorOrthogonal"
  },
  {
    "key": "ctrl+k ctrl+\\",
    "command": "-workbench.action.splitEditorOrthogonal"
  },
  {
    "key": "ctrl+b shift+5",
    "command": "workbench.action.splitEditor"
  },
  {
    "key": "ctrl+\\",
    "command": "-workbench.action.splitEditor"
  }
]